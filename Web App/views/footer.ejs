<script type="text/javascript">
(function () {
  var images = document.getElementsByClassName("hires");

  for(var i = 0; i < images.length; i++) {
    var imageSource = images[i].getAttribute("data-src");
    var imageType = imageSource.substr(-4);

    imageSource = imageSource.substr(0, imageSource.length - 4);

    if (window.devicePixelRatio == 2) {
      imageSource = (imageSource + "@2x" + imageType);
    } else {
      imageSource = (imageSource + imageType);
    }

    images[i].src = imageSource;
  }
})();
</script>
