  <meta name="description" content="The Emily Carr University Catalog of graduating student work as exhibited in their annual show.">
  <title>Emily Carr University The Show Catalog – <%= title %></title>
  <meta name="author" content="Justin Alm @ Steamclock Software">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport", content="width=device-width, initial-scale=1">

  <link rel="shortcut icon" href="/public/images/favicon.ico">
  <link rel="shortcut icon" href="/public/images/favicon.png">
  <!-- 57x57 (precomposed) for iPhone 3GS, 2011 iPod Touch and older Android devices -->
  <link rel="apple-touch-icon-precomposed" href="/public/images/apple-touch-icon-precomposed.png">
  <!-- 72x72 (precomposed) for 1st generation iPad, iPad 2 and iPad mini -->
  <link rel="apple-touch-icon-precomposed" sizes="72x72" href="/public/images/apple-touch-icon-72x72-precomposed.png">
  <!-- 114x114 (precomposed) for iPhone 4, 4S, 5 and 2012 iPod Touch -->
  <link rel="apple-touch-icon-precomposed" sizes="114x114" href="/public/images/apple-touch-icon-114x114-precomposed.png">
  <!-- 144x144 (precomposed) for iPad 3rd and 4th generation -->
  <link rel="apple-touch-icon-precomposed" sizes="144x144" href="/public/images/apple-touch-icon-144x144-precomposed.png">

  <meta name="viewport" content="initial-scale=1, maximum-scale=1">

  <link href='http://fonts.googleapis.com/css?family=Open+Sans:100,300,700' rel='stylesheet' type='text/css'>
  <link rel="stylesheet" href="/public/stylesheets/style.css" />

</head>
<body>
<div class="container-fluid">
  <div class="row-fluid">
      <div class="header">
        <a class="menu-button">&nbsp;</a>
        <div class="desktop-nav">
          <div class="logos">
            <% if (ecuad.selectedYear) { %>
            <a class="brand" title="Go to the home page of the Emily Carr University Grad Website" href="/<%= ecuad.selectedYear %>/home">
            <% } else { %>
            <a class="brand" title="Go to the home page of the Emily Carr University Grad Website" href="/">
              <% } %>
              <img class="hires ecuad-logo-top" alt="A landscape treatment of the ECUAD logo" src="/public/images/ecuad-logo.png" data-src="/public/images/ecuad-logo.png" width="155" />
            </a>
            <img class="hires the-show-logo" alt="A landscape treatment of the ECUAD logo" src="/public/images/the-show.jpg" data-src="/public/images/the-show.jpg"/>
          </div><!-- Logos -->
        <% if (ecuad.selectedYear === ecuad.activeYear) { %>
          <div class="show-tag">
            <p class="the-show">Graduation Exhibition at<br>
              Emily Carr University</p>
              <p>6-20 May 2018<br>
          </div><!-- Span2 -->
        <% } else if (ecuad.selectedYear) { %>
          <div class="show-tag">
            <p class="the-show">Graduation Exhibition at<br>
              Emily Carr University</p>
              <p>Archive of <%= ecuad.selectedYear %> Submissions<br>
              </p>
          </div><!-- Span2 -->
        <% } %>
          <div class="link-list">
            <ul class="header-nav">
              <% if (ecuad.selectedYear) { %>
                <li><a href="/about">About</a></li>
                <li><a href="/<%= ecuad.selectedYear %>/degree/design/">Design</a></li>
                <li><a href="/<%= ecuad.selectedYear %>/degree/media-arts/">Media Arts</a></li>
                <li><a href="/<%= ecuad.selectedYear %>/degree/visual-arts/">Visual Arts</a></li>
                <li><a href="/<%= ecuad.selectedYear %>/degree/mfa/">MFA</a></li>
                <li><a href="/<%= ecuad.selectedYear %>/degree/mdes/">MDes</a></li>
                <li><a href="/<%= ecuad.selectedYear %>/projects/">All</a></li>
              <% } %>
            </ul>
          </div><!-- Span6 -->
        </div><!-- Desktop-nav -->
        <div class="mobile-logos">
            <% if (ecuad.selectedYear) { %>
            <a class="brand" title="Go to the home page of the Emily Carr University Grad Website" href="/<%= ecuad.selectedYear %>/home">
            <% } else { %>
            <a class="brand" title="Go to the home page of the Emily Carr University Grad Website" href="/">
              <% } %>
              <img class="hires ecuad-logo-top" alt="A landscape treatment of the ECUAD logo" src="//:0" data-src="/public/images/ecuad-logo.png" width="155" />
            </a>
        </div><!-- Logos -->
        <div class="mobile-nav">
          <% if (ecuad.selectedYear === ecuad.activeYear) { %>
          <div class="span6">
            <p class="the-show">Graduation Exhibition at<br>
              Emily Carr University</p>
              <p>6-20 May 2018<br>
          </div><!-- Span 4 -->
          <% } else if (ecuad.selectedYear) { %>
          <div class="span6">
            <p class="the-show">The Show At<br>
              Emily Carr University</p>
              <p>Archive of <%= ecuad.selectedYear %> Submissions</p>
          </div><!-- Span 4 -->
          <% } %>
          <div class="span4">
            <ul class="header-nav">
              <% if (ecuad.selectedYear) { %>
                <li><a href="/about/">About</a></li>
                <li><a href="/<%= ecuad.selectedYear %>/degree/design/">Design</a></li>
                <li><a href="/<%= ecuad.selectedYear %>/degree/media-arts/">Media Arts</a></li>
                <li><a href="/<%= ecuad.selectedYear %>/degree/visual-arts/">Visual Arts</a></li>
                <li><a href="/<%= ecuad.selectedYear %>/degree/maa/">MAA</a></li>
                <li><a href="/<%= ecuad.selectedYear %>/degree/mdes/">MDes</a></li>
                <li><a href="/<%= ecuad.selectedYear %>/projects/">Project List</a></li>
              <% } %>
            </ul>
          </div><!-- Span4 -->
        </div><!-- Mobile-nav -->
    </div><!-- Header -->
  </div><!-- Row-fluid -->
